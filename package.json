{
  "name": "168-hours",
  "version": "1.0.0",
  "description": "Angularjs app to help you use your 168 hours per week",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install",
    "start": "http-server -a 0.0.0.0 -p 8080",
    "test": "node node_modules/karma/bin/karma start test/karma.conf.js",
    "test-single-run": "node node_modules/karma/bin/karma start test/karma.conf.js  --single-run",
    "update-webdriver": "webdriver-manager update",
    "protractor": "protractor test/protractor-conf.js",
    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + cat('bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrleszcz/168-hours.git"
  },
  "author": "John Leszczynski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrleszcz/168-hours/issues"
  },
  "homepage": "https://github.com/jrleszcz/168-hours#readme",
  "devDependencies": {
    "bower": "^1.3.1",
    "browser-sync": "^2.10.0",
    "browserify": "^12.0.1",
    "eslint": "^1.9.0",
    "faucet": "0.0.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^1.1.0",
    "gulp-sass": "^2.1.0",
    "http-server": "^0.6.1",
    "karma": "^0.12.16",
    "karma-chrome-launcher": "^0.1.4",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "~0.1.0",
    "protractor": "^2.1.0",
    "reload": "^0.7.0",
    "shelljs": "^0.2.6",
    "tape": "^4.2.2",
    "tmp": "0.0.23",
    "watchify": "^3.5.0"
  },
  "dependencies": {
    "angular": "^1.4.7"
  }
}
